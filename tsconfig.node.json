{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
    "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts","vite.config.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
